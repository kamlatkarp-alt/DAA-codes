#include <stdio.h>

int main() {
    int arr[] = {6, 19, 25, 27, 30, 37, 99, 104, 199};
    int n = sizeof(arr) / sizeof(arr[0]);
    int low = 0;
    int high = n - 1;
    int target = 378;
    int found = 0; 

    while (low <= high) {
        int mid = (low + high) / 2; 
        if (arr[mid] == target) {
            found = 1;
            break;
        } else if (target < arr[mid]) {
            high = mid - 1;
        } else {
            low = mid + 1;
        }
    }
4r
    if (found == 1) {
        printf("Element found");
    } else {
        printf("Element not found");

    }



    return 0;


}
